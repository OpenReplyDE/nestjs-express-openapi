{
  "name": "@openreplyde/nestjs-express-openapi",
  "repository": {
    "url": "https://github.com/OpenReplyDE/nestjs-express-openapi"
  },
  "version": "0.1.4",
  "description": "OpenAPI to NestJS Code Generator and Type Helpers",
  "license": "MIT",
  "main": "dist/index.cjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "bin": {
    "openapi-to-typescript": "dist/openapi-to-typescript/cli.js"
  },
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && pkgroll --clean-dist",
    "format": "npx @biomejs/biome check --write .",
    "prepack": "npm run test",
    "pretest": "npm run build",
    "test": "npm run test:spelling && npm run test:format && npm run test:lint && npm run test:specs",
    "test:format": "npx @biomejs/biome format .",
    "test:specs": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:spelling": "cspell lint --no-progress '**'",
    "test:lint": "npx @biomejs/biome lint --error-on-warnings ."
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.15.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "express": "^4.0.0 || ^5.0.0",
    "yargs": ">= 17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.24",
    "@swc/jest": "^0.2.38",
    "@swc/plugin-transform-imports": "^7.0.3",
    "@tsconfig/node20": "^20.1.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.32",
    "@types/prettier": "^3.0.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yargs": "^17.0.0",
    "cspell": "^8.19.3",
    "depcheck": "^1.4.7",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-chance": "^0.2.5",
    "pkgroll": "^2.12.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@types/prettier": "^3.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "express-jwt": "^8.5.1",
    "express-openapi-validator": "^5.5.2",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.5.3",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.7"
  }
}
